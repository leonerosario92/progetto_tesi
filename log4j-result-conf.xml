<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE log4j:configuration PUBLIC "-//log4j/log4j Configuration//EN" "log4j.dtd">
 
<log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/">
    
  
    <appender name="ResultAppender" class="org.apache.log4j.FileAppender">
       <param name="File" value="result-log.txt" />
       <layout class="org.apache.log4j.PatternLayout">
<!--           <param name="ConversionPattern" value=" %n %n %n BENCHMARK EXECUTION %n Performed at :  %d %n  %m%n"/> -->
       </layout>
    </appender>
 
    <root>
        <priority value="DEBUG"/>
        <appender-ref ref="ResultAppender" />
    </root>
     
</log4j:configuration>